openapi: 3.0.1
info:
  title: API UX Loan Operations Overview V1
  description: >-
    Consulta la linea de financiamiento y estado de planillas de una persona.
  version: "1.0.0"
servers:
  - url: '/channel/ntlc/v1'
x-bcp-api-type: UX
tags:
  - name: Loan Operations Overview
    description: Loan Operations Overview Controller
paths:
  '/loan-operations-overview/{personId}':
    get:
      tags:
        - Loan Operations Overview
      summary: >-
        Lista los datos de la linea de financiamiento de una persona.
      description: |-
        ### Acerca de la funcionalidad expuesta:
        Permite listar la linea de financiamiento de una persona.
        ### Data de Prueba:
        Ninguno.
        ### Precondiciones para el consumo de esta versión de la API:
        Ninguno.
        ### Usos válidos de Query Parameters:
        Ninguno.
        ### Variantes válidas del Payload (Cuerpo del mensaje):
        No aplica.
        ### Lista de Valores usadas en esta versión de la API:
        No aplica.
        ### Códigos de error usados en esta versión de la API:
        | Código  |  HTTP Status | Descripción |
        |---------|-------------|-------------|
        | E000400 | 400 | Los datos proporcionados no son válidos.
        | E999999 | 500 | Ocurrió un error inesperado.
      operationId: loanOperationsFinancingLineGet
      x-atlas-header:
        $ref: '#/components/schemas/LoanOperationsOverviewHeaders'
      parameters:
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/Authorization'
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/Request-ID'
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/request-date'
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/app-code'
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/caller-name'
        - $ref: "#/components/parameters/personId"
      responses:
        '200':
          description: Busqueda Exitosa.
          content:
            application/json;charset=UTF-8:
              schema:
                $ref: '#/components/schemas/LoanOperationsFinancingLineResponse'
            application/json:
              schema:
                $ref: '#/components/schemas/LoanOperationsFinancingLineResponse'
        '400':
          description: El cliente envi&oacute; datos incorrectos.
          content:
            application/json;charset=UTF-8:
              schema:
                $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/schemas/ApiException'
            application/json:
              schema:
                $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/schemas/ApiException'
        500:
          description: Ocurrió un error inesperado.
          content:
            application/json;charset=UTF-8:
              schema:
                $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/schemas/ApiException'
            application/json:
              schema:
                $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/schemas/ApiException'
  '/loan-operations-overview/payroll-status/{personId}':
    get:
      tags:
        - Loan Operations Overview
      summary: >-
        Lista los datos de planillas resumidas de una persona.
      description: |-
        ### Acerca de la funcionalidad expuesta:
        Permite listar un resumen de los estados de planillas de una persona.
        ### Data de Prueba:
        Ninguno.
        ### Precondiciones para el consumo de esta versión de la API:
        Ninguno.
        ### Usos válidos de Query Parameters:
        Ninguno.
        ### Variantes válidas del Payload (Cuerpo del mensaje):
        No aplica.
        ### Lista de Valores usadas en esta versión de la API:
        No aplica.
        ### Códigos de error usados en esta versión de la API:
        | Código  |  HTTP Status | Descripción |
        |---------|-------------|-------------|
        | E000400 | 400 | Los datos proporcionados no son válidos.
        | E999999 | 500 | Ocurrió un error inesperado.
      operationId: loanOperationsPayrollStatusGet
      x-atlas-header:
        $ref: '#/components/schemas/LoanOperationsOverviewHeaders'
      parameters:
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/Authorization'
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/Request-ID'
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/request-date'
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/app-code'
        - $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/parameters/caller-name'
        - $ref: "#/components/parameters/personId"
      responses:
        '200':
          description: Busqueda Exitosa.
          content:
            application/json;charset=UTF-8:
              schema:
                $ref: '#/components/schemas/LoanOperationsPayrollStatusResponse'
            application/json:
              schema:
                $ref: '#/components/schemas/LoanOperationsPayrollStatusResponse'
        '400':
          description: El cliente envi&oacute; datos incorrectos.
          content:
            application/json;charset=UTF-8:
              schema:
                $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/schemas/ApiException'
            application/json:
              schema:
                $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/schemas/ApiException'
        500:
          description: Ocurrió un error inesperado.
          content:
            application/json;charset=UTF-8:
              schema:
                $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/schemas/ApiException'
            application/json:
              schema:
                $ref: 'https://swaggerhub.lima.bcp.com.pe/v1/domains/BCP_Org/Atlas-Domain/1.0.0#/components/schemas/ApiException'

components:
  parameters:
    personId:
      name: personId
      in: path
      description: IDC de la persona a Consultar
      required: true
      schema:
        minLength: 9
        maxLength: 16
        type: string
        example: 430134381000
  schemas:
    LoanOperationsOverviewHeaders:
      type: object
    LoanOperationsPayrollStatusResponse:
      type: object
      description: Contiene la informacion resumidad de los estados de planillas.
      properties:
        payrollSummary:
          type: number
          example: 2
          description: cantidad de planillas.
        status:
          type: string
          example: En proceso
          description: Estado de las planillas.
        dateUpdated:
          type: string
          example: 07/08/2020 - 4:00
    LoanOperationsFinancingLineResponse:
      type: object
      description: Contiene la informacion de la linea de financiamiento.
      properties:
        adjustmentAmount:
          type: number
          x-technical-description: MAR-OLDC-AJUG-A
          example: 1000.0
          description: Monto de Ajuste
        availableAmount:
          type: number
          x-technical-description: MAR-OLDC-AJUG-A
          example: 700.0
        usedAmount:
          type: number
          x-technical-description: MAR-OCLF-MTUT-A
          example: 300.0
          description: Monto Utilizado de la Linea.
        isAvailableBalance:
          type: boolean
          example: 0
          description: Monto Utilizado de la Linea.
